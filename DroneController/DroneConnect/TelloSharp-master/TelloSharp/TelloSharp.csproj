<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Description>A C# Library For Controlling DJI Tello Drones</Description>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageIcon>icon.jpeg</PackageIcon>
    <RepositoryUrl>https://github.com/BeanFeed/TelloSharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>en</NeutralLanguage>
    <AssemblyVersion>1.0.0</AssemblyVersion>
    <FileVersion>1.0.0</FileVersion>
    <PackageTags>Drone;Tello;C#;CS;DJI</PackageTags>
    <Authors>BeanFeed</Authors>
    <PackageId>$(AssemblyName)</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Product>TelloSharp</Product>
    <Version>1.0.4</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Update="icon.jpeg">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
